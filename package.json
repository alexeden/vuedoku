{
  "name": "learning-vuex",
  "version": "1.0.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexeden/learning-vuex.git"
  },
  "scripts": {
    "builder:type-check": "tsc --noEmit --project build/tsconfig.json",
    "build": "npm run clean && webpack --config ./build/webpack.config.ts",
    "build:watch": "nodemon",
    "lint:scss": "stylelint --config .stylelintrc src/**/*.scss",
    "clean": "rimraf dist",
    "stats": "webpack --config ./build/webpack.config.ts --profile --json > build-stats.json",
    "start": "webpack-dev-server --inline --config ./build/webpack.config.ts"
  },
  "author": "Alex Eden",
  "dependencies": {
    "material-design-icons": "^3.0.1",
    "normalize.css": "^8.0.1",
    "vue": "^2.6.10",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-typescript": "^7.3.3",
    "@types/extract-text-webpack-plugin": "^3.0.4",
    "@types/html-webpack-plugin": "^3.2.0",
    "@types/node": "^12.0.3",
    "@types/ramda": "^0.26.9",
    "@types/webpack": "^4.4.32",
    "chalk": "^2.4.2",
    "circular-dependency-plugin": "^5.0.2",
    "clean-webpack-plugin": "^2.0.2",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^2.1.1",
    "extract-loader": "^3.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^5.5.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "ramda": "^0.26.1",
    "raw-loader": "^2.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^10.0.1",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-scss": "^3.6.1",
    "ts-loader": "^6.0.1",
    "ts-node": "^8.2.0",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5",
    "url-loader": "^1.1.2",
    "vue-loader": "^15.7.0",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1"
  },
  "nodemonConfig": {
    "ext": "ts,vue",
    "exec": "npm run build",
    "events": {},
    "watch": [
      "src/",
      "build/"
    ]
  }
}
